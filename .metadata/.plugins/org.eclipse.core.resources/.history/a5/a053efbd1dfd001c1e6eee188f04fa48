package br.com.fiap.tdss01;

public class Operadores {
	public static void main(String[] args) {
		int m = 4 *10;
		System.out.println("Multiplicando 4 vezes 10 é igual a " + m);
		double d = 10 / 2;
		System.out.println("Dividindo 10 por 2 é igual a " + d);
		int r = 10 % 3;
		System.out.println("O resto da divisão entre 10 e 3 é de " + r);
		int i = m++;
		System.out.println("Incrementando " + i + "em 1" + m);
		
	}
	

}
